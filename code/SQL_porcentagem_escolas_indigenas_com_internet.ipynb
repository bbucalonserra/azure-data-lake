   "source": [
    "SELECT\n",
    "  NOME_UF,\n",
    "  ROUND((SUM(CASE WHEN Internet = 1 THEN 1 ELSE 0 END) / COUNT(*)) * 100, 2) AS PORCENTAGEM_COM_INTERNET\n",
    "FROM gold.educacao_rend_escolas_joined\n",
    "WHERE \n",
    "  Localizacao_Diferenciada = 2\n",
    "GROUP BY NOME_UF\n",
    "ORDER BY PORCENTAGEM_COM_INTERNET DESC"
   ]
  }
 ],
